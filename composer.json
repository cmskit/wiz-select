{
    "name": "cmskit/wizard-select",
    "type": "library",
    "version": "0.0.0",
    "description": "A cms-kit wizard to create a selectbox or a range slider",
    "license": "GPL",
    "authors": [
        {
            "name": "Christoph Taubmann",
            "email": "info@cms-kit.com"
        }
    ],
    "homepage": "http://cms-kit.com",

    "require": {
        "php": ">=5.3.3",
        "cmskit/package-installer": "1.0.*@dev",
        "cmskit/lib-jquery-ui": "dev-master"
    },
    "extra": {
        "wizards": {
            "VARCHAR": {
                "description": "",
                "embed": "wizard:select\\noption:val[lbl](title)\\noption:..."
            },
            "INTEGER": {
                "description": "",
                "embed": "wizard:select\\nmin:0\\nmax:100"
            }
        }
    }
}
